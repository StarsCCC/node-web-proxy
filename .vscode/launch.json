  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Forward-Proxy",
    "program": "${workspaceRoot}\\lib\\proxy.js",
    "args": ["--service-install"],
    "cwd": "${workspaceRoot}"
  }, {
    "type": "node",
    "request": "launch",
    "name": "Win-Service",
    "program": "${workspaceRoot}\\service-register.js",
    "args": ["-p", "7451", "-spp", "1080"],
    "cwd": "${workspaceRoot}"
  }]
}